{
  "name": "10x-website",
  "version": "0.2.1",
  "scripts": {
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "sass --watch _includes/theme/styles:_site/assets --update",
    "sass": "sass --watch _includes/theme/styles:_site/assets --verbose",
    "build:eleventy": "eleventy",
    "build:sass": "sass  --no-source-map --style=compressed _includes/theme/styles:_site/assets --update",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "luxon": "^3.1.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "clean-css": "^5.3.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.50.1",
    "uglify-js": "^3.17.4"
  }
}
